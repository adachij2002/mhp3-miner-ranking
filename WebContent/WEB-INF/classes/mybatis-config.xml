<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias
			type="mh.miner.entity.MMine"
			alias="MMine" />
		<typeAlias
			type="mh.miner.entity.TUser"
			alias="TUser" />
		<typeAlias
			type="mh.miner.entity.TStatus"
			alias="TStatus" />
		<typeAlias
			type="mh.miner.entity.MinerRanking"
			alias="MinerRanking" />
		<typeAlias
			type="mh.miner.entity.MiningStatus"
			alias="MiningStatus" />
		<typeAlias
			type="mh.miner.action.MinerRankingSearchParam"
			alias="MinerRankingSearchParam" />
		<typeAlias
			type="mh.miner.action.MiningStatusSearchParam"
			alias="MiningStatusSearchParam" />
		<typeAlias
			type="mh.miner.action.MiningStatusUpdateParam"
			alias="MiningStatusUpdateParam" />
	</typeAliases>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="JNDI">
				<property name="data_source" value="java:/comp/env/ds/mh-ds" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource="mh/miner/entity/m_mine-mapper.xml" />
		<mapper resource="mh/miner/entity/t_user-mapper.xml" />
		<mapper resource="mh/miner/entity/t_status-mapper.xml" />
		<mapper resource="mh/miner/entity/miner-ranking-mapper.xml" />
		<mapper resource="mh/miner/entity/mining-status-mapper.xml" />
	</mappers>
</configuration>
